<!DOCTYPE suite SYSTEM "cmdlinetester.dtd">

<suite id="LoadLegacyLibraryTest with .jnilib extension on Mac OS X" timeout="6000">
 <variable name="CP" value="$TESTSJARPATH$" />
 <variable name="TEST_APP" value="-cp $CP$ org.openj9.test.loadLibrary.TestLoadLegacyLibrary" />

	<test id="LoadLegacyLibrary testcase with legacy .jnilib library extension on Mac OS X platform">
		<command>$JAVA_EXE$ $TEST_APP$</command>
		<output type="success" regex="no" showMatch="yes">Successfully reached native method in .jnilib library</output>

		<output type="failure" caseSensitive="no" regex="no">The specified path is invalid</output>
		<output type="failure" caseSensitive="no" regex="no">Could not find or load main class Files</output>
		<output type="failure" caseSensitive="no" regex="no">Unhandled Exception</output>
		<output type="failure" caseSensitive="no" regex="no">corrupt</output>
		<output type="failure" caseSensitive="yes" regex="no">Processing dump event</output>
		<output type="failure" caseSensitive="yes" regex="no">JVM requested Java dump</output>
		<output type="failure" caseSensitive="yes" regex="no">JVM requested Snap dump</output>
	</test>

</suite>
